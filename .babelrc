{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": ["transform-object-assign", "universal-import"],
  "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }]
        }]
      ]
    }
  }
}
